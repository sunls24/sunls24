---
const src = import.meta.env.UMAMI_URL
const websiteId = import.meta.env.UMAMI_ID
const domains = import.meta.env.UMAMI_DOMAINS
---

{
  websiteId && (
    <script
      is:inline
      defer
      id="umami"
      src={src}
      data-website-id={websiteId}
      data-domains={domains}
    />
  )
}
